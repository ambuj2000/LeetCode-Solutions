	
#include<iostream.h>
#include<conio.h>
void main()
{
    int i,n,r,s=0;
    clrscr();
     
    cout<<"\n Enter The Number:";
    cin>>n;
     
    for(i=n;i>0; )
    {
        r=i%10;
        s=s*10+r;
        i=i/10;
    }
    
    if(s==n)
    {
        cout<<n<<" is a Palindrome Number";
    }
    else
    {
        cout<<n<<" is a not Palindrome Number";
    }
    getch();
}
